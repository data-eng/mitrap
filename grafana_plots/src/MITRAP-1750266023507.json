          "query": "import \"strings\"\n\nfrom(bucket: \"mitrap006\")\n  |> range(start: v.timeRangeStart, stop: v.timeRangeStop)\n  |> filter(fn: (r) => r._measurement == \"smps_data\")\n  |> filter(fn: (r) => strings.hasPrefix(v: r._field, prefix: \"nm\"))  \n  |> map(fn: (r) => ({\n      r with\n      nm: float(v: strings.replaceAll(v: strings.replaceAll(v: r._field, t: \"nm\", u: \"\"), t: \"_\", u: \".\")) \n  }))\n  |> map(fn: (r) => ({\n      r with nm_bin: float(v: uint(v: r.nm / 10.0)) * 10.0\n  }))\n  //|> aggregateWindow(every: 1m, fn: mean)\n  //|> group(columns: [\"_time\", \"nm_bin\"])\n  |> keep(columns: [\"_time\", \"_value\", \"nm_bin\"])\n  |> yield(name: \"table\")",
          "refId": "A"
        }
      ],
      "title": "Initial attempt at heatmap",
      "transformations": [
        {
          "id": "groupingToMatrix",
          "options": {
            "columnField": "nm_bin",
            "rowField": "_time",
            "valueField": "_value"
          }
        }
      ],
      "type": "status-history"
